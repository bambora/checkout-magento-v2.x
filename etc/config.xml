<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <payment>
            <bambora_checkout>
                <active>0</active>
                <model>Bambora\Online\Model\Method\Checkout\Payment</model>
                <title>Worldline Online Checkout</title>
                <paymentwindowid>1</paymentwindowid>
                <instantcapture>0</instantcapture>
                <instantinvoice>0</instantinvoice>
                <instantinvoicemail>0</instantinvoicemail>
                <immediateredirecttoaccept>0</immediateredirecttoaccept>
                <addsurchargetopayment>0</addsurchargetopayment>
                <surchargemode>surcharge_order_line</surchargemode>
                <sendmailorderconfirmation>1</sendmailorderconfirmation>
                <remoteinterface>1</remoteinterface>
                <order_status>processing</order_status>
                <masscaptureinvoicemail>1</masscaptureinvoicemail>
                <sort_order>1</sort_order>
                <windowstate>1</windowstate>
                <roundingmode>round_default</roundingmode>
                <uncancelorderlines>0</uncancelorderlines>
                <allowlowvalueexemption>0</allowlowvalueexemption>
                <limitlowvalueexemption>0</limitlowvalueexemption>
                <group>bambora</group>
            </bambora_checkout>
        </payment>
    </default>
</config>